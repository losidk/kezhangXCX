/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}
.fl{
	float: left;
}
.fr{
	float: right;
}
.clearfix:after{
	content:'';
	display:block;
	height:0;
	clear:both;
	visibility:none;
}
/*删除button默认边框*/
button[type="default"]{
	background-color: #BAA99A;
	border-radius: 0;
	color:#fff;
}
button::after{
	border:none;
}
